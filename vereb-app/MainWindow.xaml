<Window x:Class="vereb_app.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:vereb_app"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
	<Grid>
		

		<Canvas Margin="0,1,-4,-1" Name="canvas">
			<Canvas.Background>
				<ImageBrush ImageSource="/background.jpg"/>
			</Canvas.Background>
			<Image x:Name="bird" Source="/bird.png" Height="50" Width="50" Canvas.Left="4" Canvas.Top="149" >
			</Image>
			<Rectangle x:Name="columnUp" Height="183" Canvas.Left="272" Fill="#FFCE2F2F" Stroke="#FFCE2F2F" Canvas.Top="5" Width="57" HorizontalAlignment="Left" VerticalAlignment="Center"/>
			<Rectangle x:Name="columnDown" Height="183" Canvas.Left="271" Fill="#FFCE2F2F" Stroke="#FFCE2F2F" Canvas.Top="252" Width="57" HorizontalAlignment="Left" VerticalAlignment="Top"/>

		</Canvas>
	</Grid>
</Window>
