<Window x:Class="vereb_app.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:vereb_app"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
	<Grid>


		<Canvas Margin="0,1,-4,-1" Name="canvas">
			<Canvas.Background>
				<ImageBrush ImageSource="/background.jpg"/>
			</Canvas.Background>
			<Image x:Name="bird" Source="/bird.png" Height="50" Width="50" Canvas.Left="4" Canvas.Top="149" >
			</Image>
			<Rectangle x:Name="columnUp" Height="175" Canvas.Left="272" Stroke="#FF307522" Canvas.Top="-57" Width="57" HorizontalAlignment="Center" VerticalAlignment="Top" Fill="#FF307522"/>
			<Rectangle x:Name="columnDown" Height="431" Canvas.Left="271" Fill="#FF307522" Stroke="#FF307522" Canvas.Top="334" Width="57" HorizontalAlignment="Center" VerticalAlignment="Top"/>

		</Canvas>
	</Grid>
</Window>
